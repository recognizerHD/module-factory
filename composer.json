{
  "name": "minionfactory/module-factory",
  "description": "My implementation of modules within Laravel.",
  "type": "package",
  "keywords": [
    "laravel",
    "modules",
    "minionfactory"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Paul Warren",
      "email": "paulwarren.php@gmail.com",
      "role": "Developer",
      "homepage": "https://paulwarren.ca"
    }
  ],
  "require": {
    "php": "^8.3",
    "laravel/framework": "^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "MinionFactory\\ModuleFactory\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MinionFactory\\ModuleFactory\\ModuleServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
